{
  "name": "cz-conventional-commit-gitmoji",
  "version": "0.0.0-semantically-released",
  "description": "Commitizen adapter following the conventional-changelog format with gitmoji.",
  "type": "module",
  "main": "./index.js",
  "exports": {
    "import": "./index.js",
    "require": "./index.cjs"
  },
  "files": [
    "index.js",
    "index.cjs",
    "src"
  ],
  "scripts": {
    "test": "mocha",
    "coverage": "c8 npm run test",
    "lint": "prettier --check **/*.{js,cjs} && eslint",
    "format": "prettier --write **/*.{js,cjs} && eslint --fix",
    "release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NathanAlcantara/cz-conventional-commit-gitmoji.git"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 20"
  },
  "author": "Nathan Alcantara <nathan.g.alcantara@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.4.1",
    "gitmojis": "^3.14.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.foreach": "^4.5.0",
    "lodash.includes": "^4.3.0",
    "lodash.map": "^4.6.0",
    "word-wrap": "^1.2.5"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/js": "^9.17.0",
    "c8": "^10.1.3",
    "chai": "^5.1.2",
    "conventional-changelog-gitmoji-config": "^1.5.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-mocha": "^10.5.0",
    "globals": "^15.14.0",
    "mocha": "^11.0.1",
    "prettier": "^3.4.2",
    "semantic-release": "^24.2.0"
  },
  "overrides": {
    "semantic-release": {
      "@semantic-release/commit-analyzer": "^10.0.4",
      "@semantic-release/release-notes-generator": "^11.0.7"
    }
  }
}
